{
	"openapi": "3.0.0",
		"components": {
			"securitySchemes": {
				"basicAuth": {
					"type": "http",
					"scheme": "basic"
				}
			}
		}
	,
	"info": {
		"title": "Sample API",
		"description": "Optional multiline or single-line description in [CommonMark](http://commonmark.org/help/) or HTML.",
		"version": "0.1.9"
	},
	"paths": {
		"/compute/list" : {
			"get": {
		"summary": "Returns a list of users.",
		"description": "Optional extended description in CommonMark or HTML",
		"responses": {
			"200": {
				"description": "A JSON array of user names",
				"content": {
					"application/json": {
						"schema": {
							"type": "array",
							"items": {
								"type": "string"
							}
						}
					}
				}
			}
		}
	}
		},
		"/compute/create": {
			
			"post": {
				"security": [
					{
						"basicAuth": []
					}
				], 
				"summary": "Creating Server Compute.",
				
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"required": [
									"userName",
									"age"
								
								],
								"properties": {
									
									"username": {
										"type": "string"
									},
									"age": {
										"type": "number"
									},
									"Student": {
										"type": "object",
										"required" : ["Name"],
										"properties": {
											"Name": {
												"type": "string"
											}
										}
									}
								}
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Created"
					}
				}
			}
		}
	}
}